define(action,%arg1) // change action here

[ENABLE]
alloc(MobControl,128)

MobControl:
mov [edi+000004C0],action
mov eax,[edi+000004C0]
jmp 02656A20+6

02656A20:
jmp MobControl
db 90

[DISABLE]
02656A20: // CVecCtrlMob::WorkUpdateActive: E9 ? ? ? ? 8B ? ? ? 00 00 83 ? ? 0F ? ? ? ? ? FF [Address below]
mov eax,[edi+000004C0]

dealloc(MobControl)
